{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "txtar",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment_section"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "file_entry"
          }
        }
      ]
    },
    "comment_section": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "comment_line"
      }
    },
    "comment_line": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\n]*\\n?"
        }
      }
    },
    "file_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker",
          "content": {
            "type": "SYMBOL",
            "name": "file_marker"
          }
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "file_content_line"
            }
          }
        }
      ]
    },
    "file_marker": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "marker_start",
          "content": {
            "type": "SYMBOL",
            "name": "marker_start"
          }
        },
        {
          "type": "FIELD",
          "name": "filename",
          "content": {
            "type": "SYMBOL",
            "name": "filename"
          }
        },
        {
          "type": "FIELD",
          "name": "marker_end",
          "content": {
            "type": "SYMBOL",
            "name": "marker_end"
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "marker_start": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "--"
          },
          {
            "type": "PATTERN",
            "value": "[ \\t]*"
          }
        ]
      }
    },
    "marker_end": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[ \\t]*"
          },
          {
            "type": "STRING",
            "value": "--"
          }
        ]
      }
    },
    "filename": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^-\\n][^-\\n]*[^-\\n]|[^-\\n]"
        }
      }
    },
    "file_content_line": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\n]*\\n?"
        }
      }
    }
  },
  "extras": [
    {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "PATTERN",
                "value": "[^\\n]*"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "\\s"
          }
        ]
      }
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}